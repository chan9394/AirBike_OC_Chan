#import "EnterBtn.h"

@implementation EnterBtn

- (CGRect)titleRectForContentRect:(CGRect)contentRect {
    CGSize size = CGSizeMake(50, 44);
    CGFloat x = contentRect.size.width / 2 - size.width;
    return CGRectMake(x, 0, size.width, size.height);
};

- (CGRect)imageRectForContentRect:(CGRect)contentRect {
    CGSize size = CGSizeMake(80,20);
    CGFloat x = contentRect.size.width / 2 - 10;
    return CGRectMake(x, 13, size.width, size.height);
}
@end
